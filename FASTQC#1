#FASTQC of SRA reads

#Interactive session
qrsh -pe threads 2 -1 mem=4G

#Quality examination
cd stewartii
cd 1_analysis
mkdir 1_fastqc
cd 1_fastqc
fastqc -t 2 -o . ../../raw_data/SRR2033699.fastq
fastqc -t 2 -o . ../../raw_data/SRR2033686.fastq
fastqc -t 2 -o . ../../raw_data/SRR2033675.fastq
#copy files to computer
